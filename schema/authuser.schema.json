{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$anchor": "authuser",
  "description": "Extra user properties related to auth",
  "$merge": {
    "with": {
      "properties": {
        "authType": {
          "description": "Types of authentication used with this user",
          "type": "string",
          "isReadOnly": true
        }
      },
      "required": ["authType"]
    }
  }
}
